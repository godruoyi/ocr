{
    "name": "godruoyi/ocr",
    "description": "The Best Image Ocr SDK For BAT.",
    "keywords": ["OCR", "文字识别", "百度 OCR", "腾讯 OCR", "阿里云 OCR"],
    "homepage": "https://github.com/godruoyi/ocr",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "godruoyi",
        "email": "godruoyi@gmail.com"
    }],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.2",
        "monolog/monolog": "^3.0",
        "psr/log": "^2.0 || ^3.0",
        "godruoyi/easy-container": "^2.0 || ^3.0",
        "psr/simple-cache": "^2.0 || ^3.0",
        "symfony/cache": "^6.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "mockery/mockery": "^1.6",
        "laravel/pint": "^1.10",
        "ext-gd": "*"
    },
    "autoload": {
        "psr-4": {
            "Godruoyi\\OCR\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "pint": "vendor/bin/pint"
    }
}
